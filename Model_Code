        "# @title\n",
        "#This function downloads relevant data from Yahoo Finance and calculates the percent returns necessary to conduct risk analysis\n",
        "def get_percent_returns(stocks,positions,units,start_date,end_date,data=False):\n",
        "  if data==False:\n",
        "    portfolio_temp_df = pd.DataFrame(index=yf.download(stocks[0], start=start_date, end=end_date).index,columns=['Portfolio'],data=0)\n",
        "    for stock, unit, position in zip(stocks,units,positions):\n",
        "      if (position == 'Long') or (position=='Short'):\n",
        "        data_df = yf.download(stock, start=start_date, end=end_date).rename(columns={'Adj Close':stock})[stock]*unit\n",
        "      elif position=='Call':\n",
        "        data_temp_df = yf.download(stock, start=start_date, end=end_date).rename(columns={'Adj Close':stock})[stock]\n",
        "        data_df = pd.DataFrame(index=data_temp_df.index)\n",
        "        data_df[f'{stock}'] = [black_scholes_option(stock_price, stock_price, T=unit[2], r=unit[3], sigma=unit[1], option_type='Call') * unit[0] for stock_price in data_temp_df]\n",
        "      elif position=='Put':\n",
        "        data_temp_df = yf.download(stock, start=start_date, end=end_date).rename(columns={'Adj Close':stock})[stock]\n",
        "        data_df = pd.DataFrame(index=data_temp_df.index)\n",
        "        data_df[f'{stock}'] = [black_scholes_option(stock_price, stock_price, T=unit[2], r=unit[3], sigma=unit[1], option_type='Put') * unit[0] for stock_price in data_temp_df]\n",
        "      portfolio_temp_df = pd.merge(portfolio_temp_df, data_df, left_index=True, right_index=True)\n",
        "      portfolio_temp_df[f'{stock}_Pct_Returns'] = portfolio_temp_df[stock].pct_change()\n",
        "      portfolio_temp_df['Portfolio'] += portfolio_temp_df[stock]\n",
        "      portfolio_temp_df['Port_Pct_Returns'] = portfolio_temp_df['Portfolio'].pct_change()\n",
        "  else:\n",
        "    portfolio_temp_df = pd.DataFrame(index=pd.to_datetime(data[0]['Dates'], format='%m/%d/%Y'),columns=['Portfolio'],data=0)\n",
        "    for stock,stock_data, unit, position in zip(stocks,data,units,positions):\n",
        "      if (position == 'Long') or (position=='Short'):\n",
        "        data_df = pd.DataFrame(index=portfolio_temp_df.index,columns=[stock],data=0)\n",
        "        data_df[stock] = (stock_data['PX_LAST']*unit).to_list()\n",
        "      elif position=='Call':\n",
        "        data_temp_df = pd.DataFrame(index=portfolio_temp_df.index,columns=[stock],data=0)\n",
        "        data_temp_df[stock] = (stock_data['PX_LAST']).to_list()\n",
        "        data_df = pd.DataFrame(index=data_temp_df.index)\n",
        "        data_df[f'{stock}'] = [black_scholes_option(stock_price, stock_price, T=unit[2], r=unit[3], sigma=unit[1], option_type='Call') * unit[0] for stock_price in data_temp_df]\n",
        "      elif position=='Put':\n",
        "        data_temp_df = pd.DataFrame(index=portfolio_temp_df.index,columns=[stock],data=0)\n",
        "        data_temp_df[stock] = (stock_data['PX_LAST']).to_list()\n",
        "        data_df = pd.DataFrame(index=data_temp_df.index)\n",
        "        data_df[f'{stock}'] = [black_scholes_option(stock_price, stock_price, T=unit[2], r=unit[3], sigma=unit[1], option_type='Put') * unit[0] for stock_price in data_temp_df]\n",
        "      portfolio_temp_df = pd.merge(portfolio_temp_df, data_df, left_index=True, right_index=True)\n",
        "      portfolio_temp_df[f'{stock}_Pct_Returns'] = portfolio_temp_df[stock].pct_change()\n",
        "      portfolio_temp_df['Portfolio'] += portfolio_temp_df[stock]\n",
        "      portfolio_temp_df['Port_Pct_Returns'] = portfolio_temp_df['Portfolio'].pct_change()\n",
        "  for stock in stocks:\n",
        "    portfolio_temp_df[f'{stock}_Proportion'] = portfolio_temp_df[stock]/portfolio_temp_df['Portfolio']\n",
        "  return portfolio_temp_df['Port_Pct_Returns'][1:],[portfolio_temp_df[f'{stock}_Pct_Returns'][1:] for stock in stocks],[portfolio_temp_df[f'{stock}_Proportion'][1:] for stock in stocks],[portfolio_temp_df[f'{stock}'][1:] for stock in stocks]\n",
        "\n",
        "#This function finds the sample mean of daily returns for a specified period. Necessary for fitting GBM using windowing\n",
        "def get_mewbar_window(years,percent_returns,back=252*10):\n",
        "  mewbar = []\n",
        "  for i in range(len(percent_returns)-back):\n",
        "    mewbar.extend([sum(percent_returns[(back+i-years*252):(back+i)])/(years*252)])\n",
        "  return mewbar\n",
        "\n",
        "#This function does the same as the one above, but for GBM using exponential weighting\n",
        "def get_mewbar_expon(lamd, percent_returns,back=252*10):\n",
        "    mewbar = []\n",
        "    mew_0 = 0\n",
        "    denominator = 1/(1-lamd)\n",
        "    for i in range(back):\n",
        "      mew_0 += percent_returns[i]*lamd**(back-i-1)\n",
        "    mewbar.extend([mew_0/denominator])\n",
        "    for j in range(len(percent_returns)-back-1):\n",
        "      mew_0 = mew_0*lamd\n",
        "      mew_0 += percent_returns[j+252*10]\n",
        "      mewbar.extend([mew_0/denominator])\n",
        "    return mewbar\n",
        "\n",
        "#This function finds the sample standard deviaiton of daily returns for a specified period. Necessary for fitting GBM using windowing\n",
        "def get_sdbar_window(percent_returns,years,back=252*10):\n",
        "  sdbar = []\n",
        "  for i in range(len(percent_returns)-back):\n",
        "    sdbar.extend([statistics.pstdev(percent_returns[(back-252*years+i):(i+back)])])\n",
        "  return sdbar\n",
        "\n",
        "#This function does the same as the one above, but for GBM using exponential weighting\n",
        "def get_sdbar_expon(lamd, percent_returns,back=252*10):\n",
        "  mew2_0 = 0\n",
        "  denominator = 1/(1-lamd)\n",
        "  for i in range(back):\n",
        "    mew2_0 += percent_returns[i]**2*lamd**(back-i-1)\n",
        "  mew2 = [mew2_0/denominator]\n",
        "  for j in range(len(percent_returns)-back-1):\n",
        "    mew2_0 = mew2_0*lamd\n",
        "    mew2_0 += percent_returns[j+back]**2\n",
        "    mew2.extend([mew2_0/denominator])\n",
        "  mu = get_mewbar_expon(lamd=lamd, percent_returns=percent_returns,back=252*10)\n",
        "  vol = [np.sqrt(mew2[i] - mu[i]**2) for i in range(len(mu))]\n",
        "  return vol\n",
        "\n",
        "#Using the time series of sample mean and sd, this function calculates GBM drift and volatility parameters\n",
        "def get_gbm(percent_returns,years=False,back=252*10,lamb=False):\n",
        "  if lamb == False:\n",
        "    means = get_mewbar_window(percent_returns=percent_returns,years=years,back=back)\n",
        "    vols = get_sdbar_window(percent_returns=percent_returns,years=years,back=back)\n",
        "  else:\n",
        "    means = get_mewbar_expon(lamd=lamb, percent_returns=percent_returns,back=252*10)\n",
        "    vols = get_sdbar_expon(lamd=lamb,percent_returns=percent_returns,back=back)\n",
        "  return [x * np.sqrt(252) for x in vols], [252*x + (y*np.sqrt(252))**2/2 for x, y in zip(means, vols)]"
      ],
      "metadata": {
        "id": "LjxPR_2uXa9x",
