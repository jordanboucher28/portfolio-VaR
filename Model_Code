      "cell_type": "code",
      "source": [
        "# @title\n",
        "#This function calculates parametric VaR using GBM parameters found using either windowing or exponential weighting\n",
        "def get_VaR(percent_returns, cash, years=False,p=.99,back=252*10,lamb=False):\n",
        "  gbm_vols, gbm_means = get_gbm(percent_returns=percent_returns, years=years,back=back,lamb=lamb)\n",
        "  T = 5/252\n",
        "  return [cash-cash*np.exp(gbm_vols[i]*np.sqrt(T)*stats.norm.ppf(1-p)+(gbm_means[i]-gbm_vols[i]**2/2)*T) for i in range(len(percent_returns)-back)]\n",
        "\n",
        "#This function calculates parametric CVaR using GBM parameters found using either windowing or exponential weighting\n",
        "def get_ES(percent_returns, cash, years=False,p=.975,back=252*10,lamb=False):\n",
        "  gbm_vols, gbm_means = get_gbm(percent_returns=percent_returns, years=years,back=back,lamb=lamb)\n",
        "  T = 5/252\n",
        "  norm = [stats.norm.cdf(stats.norm.ppf(1-p)-gbm_vols[i]*np.sqrt(T)) for i in range(len(percent_returns)-back)]\n",
        "  return [cash*(1-norm[i]*np.exp(gbm_means[i]*T)/(1-p)) for i in range(len(percent_returns)-back)]\n",
        "\n",
        "#This function calculates parametric VaR using GBM parameters found using either windowing or exponential weighting for a short postion in the security\n",
        "def get_short_VaR(percent_returns, cash, years=False,p=.99,back=252*10,lamb=False):\n",
        "  gbm_vols, gbm_means = get_gbm(percent_returns=percent_returns, years=years,back=back,lamb=lamb)\n",
        "  T = 5/252\n",
        "  return [-1*(cash-cash*np.exp(gbm_vols[i]*np.sqrt(T)*stats.norm.ppf(1-(1-p))+(gbm_means[i]-gbm_vols[i]**2/2)*T) )for i in range(len(percent_returns)-back)]\n",
        "\n",
        "#This function calculates parametric CVaR using GBM parameters found using either windowing or exponential weighting for a short postion in the security\n",
        "def get_short_ES(percent_returns, cash, years=False,p=.975,back=252*10,lamb=False):\n",
        "  ES_1 = get_ES(percent_returns=percent_returns,years=years,p=0,cash=cash,back=back,lamb=lamb)\n",
        "  ES_2 = get_ES(percent_returns=percent_returns,years=years,p=(1-p),cash=cash,back=back,lamb=lamb)\n",
        "  return [(-1*ES_1[i]+p*ES_2[i])/(1-p) for i in range(len(percent_returns)-back)]\n",
        "\n",
        "#This function calculates Monte Carlo Var, which we calculate seperately for each stock (as opposed to the entire portfolio) to allow more flexibility in our asset positions\n",
        "def get_MC_VaR_ES(percent_returns_lst, stock_proportions_lst,position_values_lst, cash, units, positions, sims, years=False,p=.99,back=252*10,lamb=False):\n",
        "  gbm_params_temp = [get_gbm(percent_returns=percent_returns, years=years,back=back,lamb=lamb) for percent_returns in percent_returns_lst]\n",
        "  gbm_means = pd.DataFrame([gbm_params_temp[i][1] for i in range(len(gbm_params_temp))]).transpose()\n",
        "  gbm_vols = pd.DataFrame([gbm_params_temp[i][0] for i in range(len(gbm_params_temp))]).transpose()\n",
        "  gbm_corr = np.corrcoef(percent_returns_lst)\n",
        "  stock_proportions_df = pd.DataFrame(stock_proportions_lst).transpose().iloc[len(stock_proportions_lst[0])-len(gbm_means):]\n",
        "  T = 5/252\n",
        "  VaR_temp = []; ES_temp=[]; VaR_short_temp=[];ES_short_temp=[]\n",
        "  for (index1, mean_row),(index2,vol_row),(index3,proportion_row) in zip(gbm_means.iterrows(),gbm_vols.iterrows(),stock_proportions_df.iterrows()):\n",
        "    portfolio_return_temp_df = pd.DataFrame(index=range(sims))\n",
        "    if len(percent_returns_lst)>1:\n",
        "      W5_array = multivariate_normal(mean = [0 for i in range(len(percent_returns_lst))],cov=gbm_corr,size=sims)\n",
        "    else:\n",
        "      W5_array = normal(loc=0,scale=1,size=sims)\n",
        "      W5_array = np.array([np.array([x]) for x in W5_array])\n",
        "    for mean,vol,proportion,i,position in zip(mean_row,vol_row,proportion_row,range(len(mean_row)),positions):\n",
        "      if position == \"Long\":\n",
        "        portfolio_return_temp_df[f'{i}_Losses'] = cash*proportion-cash*proportion*(np.exp((mean-vol**2/2)*T+vol*np.sqrt(T)*np.array([sub_array[i] for sub_array in W5_array])))\n",
        "      elif position == \"Short\":\n",
        "        portfolio_return_temp_df[f'{i}_Losses'] = -cash*proportion+cash*proportion*(np.exp((mean-vol**2/2)*T+vol*np.sqrt(T)*np.array([sub_array[i] for sub_array in W5_array])))\n",
        "      elif (position == \"Call\") or (position=='Put'):\n",
        "        portfolio_return_temp_df[f'{i}_pct_of_original'] = np.exp((mean-vol**2/2)*T+vol*np.sqrt(T)*np.array([sub_array[i] for sub_array in W5_array]))\n",
        "        old_option_price = position_values_lst[i].loc[index3]/units[i][0]\n",
        "        old_stock_price = calculate_stock_price(old_option_price,units[i][3],units[i][1],units[i][2],position)\n",
        "        portfolio_return_temp_df[f'{i}_new_option_price'] = [black_scholes_option(old_stock_price*pct_orig, old_stock_price, T=units[i][2]-5/252, r=units[i][3], sigma=units[i][1], option_type=position) for pct_orig in portfolio_return_temp_df[f'{i}_pct_of_original']]\n",
        "        portfolio_return_temp_df[f'{i}_pct_change_option'] = (portfolio_return_temp_df[f'{i}_new_option_price']-old_option_price)/old_option_price\n",
        "        portfolio_return_temp_df[f'{i}_Losses'] = cash*proportion*portfolio_return_temp_df[f'{i}_pct_change_option']\n",
        "        portfolio_return_temp_df = portfolio_return_temp_df.drop(columns=[f'{i}_new_option_price',f'{i}_pct_change_option',f'{i}_pct_of_original'])\n",
        "    portfolio_return_temp_df['Portfolio_Losses'] = portfolio_return_temp_df.sum(axis=1)\n",
        "    portfolio_return_temp_df['Short_Portfolio_Losses'] = -1 * portfolio_return_temp_df['Portfolio_Losses']\n",
        "    VaR_temp.extend([np.quantile(portfolio_return_temp_df['Portfolio_Losses'],.99)]);ES_temp.extend([sum(portfolio_return_temp_df['Portfolio_Losses'][portfolio_return_temp_df['Portfolio_Losses'] >= np.quantile(portfolio_return_temp_df['Portfolio_Losses'],.975)])/len(portfolio_return_temp_df['Short_Portfolio_Losses'][portfolio_return_temp_df['Portfolio_Losses'] >= np.quantile(portfolio_return_temp_df['Portfolio_Losses'],.975)])])\n",
        "    VaR_short_temp.extend([np.quantile(portfolio_return_temp_df['Short_Portfolio_Losses'],.99)]);ES_short_temp.extend([sum(portfolio_return_temp_df['Short_Portfolio_Losses'][portfolio_return_temp_df['Short_Portfolio_Losses'] >= np.quantile(portfolio_return_temp_df['Short_Portfolio_Losses'],.975)])/len(portfolio_return_temp_df['Short_Portfolio_Losses'][portfolio_return_temp_df['Short_Portfolio_Losses'] >= np.quantile(portfolio_return_temp_df['Short_Portfolio_Losses'],.975)])])\n",
        "    #if index1%10==0: print(np.quantile(portfolio_return_temp_df['Portfolio_Losses'],.99))\n",
        "  return VaR_temp,ES_temp,VaR_short_temp,ES_short_temp\n",
        "\n",
        "#This is a simple function used for pricing options under Black Scholes\n",
        "def black_scholes_option(S, K, T, r, sigma, option_type='Put'):\n",
        "    d1 = (math.log(S / K) + (r + 0.5 * sigma**2) * T) / (sigma * math.sqrt(T))\n",
        "    d2 = d1 - sigma * math.sqrt(T)\n",
        "    if option_type == 'Put':\n",
        "        option_price = K * math.exp(-r * T) * norm.cdf(-d2) - S * norm.cdf(-d1)\n",
        "    elif option_type == 'Call':\n",
        "        option_price = S * norm.cdf(d1) - K * math.exp(-r * T) * norm.cdf(d2)\n",
        "    else:\n",
        "        raise ValueError(\"Invalid option_type. Use 'Put' or 'Call'.\")\n",
        "    return option_price\n",
        "\n",
        "#This is an inverse of the above equation, where options price is an input and stock price is the output\n",
        "def calculate_stock_price(option_price, interest_rate, volatility, time_to_maturity, option_type='Call'):\n",
        "    d1 = ((interest_rate + 0.5 * volatility**2) * time_to_maturity) / (volatility * math.sqrt(time_to_maturity))\n",
        "    d2 = d1 - volatility * math.sqrt(time_to_maturity)\n",
        "    if option_type == 'Put':\n",
        "        stock_price = option_price / (-norm.cdf(-d1) + math.exp(-interest_rate * time_to_maturity) * norm.cdf(-d2))\n",
        "    elif option_type == 'Call':\n",
        "        stock_price = option_price / (norm.cdf(d1) - math.exp(-interest_rate * time_to_maturity) * norm.cdf(d2))\n",
        "    else:\n",
        "        raise ValueError(\"Invalid option_type. Use 'Put' or 'Call'.\")\n",
        "    return stock_price\n",
        "\n",
        "#This is a function which takes a pandas series as an input and returns the mean of the largest 1-p values\n",
        "def custom_rolling_mean(series,p=.975):\n",
        "    top_percent = series.nlargest(int(len(series) * (1-p)))\n",
        "    return top_percent.mean()\n",
        "\n",
        "#This function calculates all types of historical VaR. Note that it is only totally accuracte for portfolios of stocks (as opposed to options)\n",
        "def get_historical_VaR(position_values_lst, cash, positions, years, p=.99, back=252*10):\n",
        "  position_values_df = pd.DataFrame(position_values_lst).transpose().set_index(position_values_lst[0].index)[5:]\n",
        "  dollar_changes_df = pd.DataFrame(index=position_values_lst[0].index,columns=[f'{series.name} losses' for series in position_values_lst],data=0).iloc[5:]\n",
        "  for dollar_position, direction_position,col in zip(position_values_lst,positions,dollar_changes_df):\n",
        "    if (direction_position == 'Long') or (direction_position == 'Call') or (direction_position == 'Put'):\n",
        "      dollar_changes_df[col] = dollar_position[:-5].values-dollar_position[5:].values\n",
        "    elif direction_position == 'Short':\n",
        "      dollar_changes_df[col] = dollar_position[5:].values-dollar_position[:-5].values\n",
        "  dollar_changes_df['Portfolio_losses'] = dollar_changes_df.sum(axis=1)\n",
        "  dollar_changes_df['Adjusted_Portfolio_losses'] = [dollar_changes_df['Portfolio_losses'].values[i]/position_values_df.sum(axis=1).values[i]*10000 for i in range(len(dollar_changes_df['Portfolio_losses']))]\n",
        "  dollar_changes_df['long_VaR'] = dollar_changes_df['Adjusted_Portfolio_losses'].rolling(window=years*252).quantile(0.99)\n",
        "  dollar_changes_df['short_VaR'] = (-1*dollar_changes_df['Adjusted_Portfolio_losses']).rolling(window=years*252).quantile(0.99)\n",
        "  dollar_changes_df['long_ES'] = dollar_changes_df['Adjusted_Portfolio_losses'].rolling(window=years*252).apply(custom_rolling_mean)\n",
        "  dollar_changes_df['short_ES'] = (-1*dollar_changes_df['Adjusted_Portfolio_losses']).rolling(window=years*252).apply(custom_rolling_mean)\n",
        "  return dollar_changes_df['long_VaR'].iloc[len(dollar_changes_df['long_VaR'])-len(position_values_lst[0])+back:].to_list(),dollar_changes_df['long_ES'].iloc[len(dollar_changes_df['long_VaR'])-len(position_values_lst[0])+back:].to_list(),dollar_changes_df['short_VaR'].iloc[len(dollar_changes_df['long_VaR'])-len(position_values_lst[0])+back:].to_list(),dollar_changes_df['short_ES'].iloc[len(dollar_changes_df['long_VaR'])-len(position_values_lst[0])+back:].to_list()"
      ],
      "metadata": {
        "id": "YukrhCFUn-qP",
        "cellView": "form"
      },
      "execution_count": 3,
      "outputs": []
